{
  "name": "nuxtjs-template",
  "private": "true",
  "description": "Template for public websites using nuxt 3",
  "author": "Julien Sulpis",
  "scripts": {
    "analyze": "nuxt analyze",
    "build": "nuxt build",
    "build:mock": "cross-env MOCK=true pnpm build",
    "clean": "rimraf .nuxt .output dist playwright-report",
    "dev": "nuxt dev",
    "dev:mock": "cross-env MOCK=true pnpm dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "postinstall": "husky install && nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "preview": "nuxt preview",
    "serve": "run-s build preview",
    "serve:mock": "run-s build:mock preview",
    "pretest": "playwright install --with-deps",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "typecheck": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@vueuse/nuxt": "9.12.0",
    "nuxt": "3.1.2",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-vue": "9.9.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "sass": "1.58.0",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vue-tsc": "1.0.24"
  }
}
